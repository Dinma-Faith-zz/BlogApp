<h2>Display Users Posts</h2>

<%= render partial: "users/user" , locals: { user: @user } %>
 
<div class="post-container">
  <% @posts.each_with_index do |post, index| %>
    <%= render partial: "posts/post" , locals: { post: post, index: index } %>
      <% end %>

      <%= link_to "Back to user", users_path(@user) %>


</div>
